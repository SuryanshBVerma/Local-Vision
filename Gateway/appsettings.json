{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "images": {
        "ClusterId": "imagesCluster",
        "Match": { "Path": "/images/{**catch-all}" },
        "Transforms": [
          { "PathRemovePrefix": "/images" }
        ]
      }
    },
    "Clusters": {
      "imagesCluster": {
        "Destinations": {
          "imagesApi": { "Address": "http://localhost:7001/api" } // [HTTP] Unsecured connection to Images API
        }
      }
    }
  }
}
